{
	"name": "subbind",
	"version": "1.0",
	"manifest_version": 3,
	"background": {"service_worker": "background.js"},




  
 "content_scripts": [
   {
     "matches": ["*://*.bindingdb.org/*"],
     "js": [
	 "jquery.js",
	 "script.js"
	 ],
	 "run_at": "document_idle"
   }
 ],

  
 
  
    "options_ui": {
    "page": "options.html",
    "browser_style": false
  },
  "browser_action": {
    "default_title": "on",
    "default_icon": {
      "64": "icons/logo.png"
    }
  },
  "permissions" : [
    "contextMenus",
    "tabs"
   ],
   "host_permissions": [
  "http://*/*",
  "https://*/*"
]


}
